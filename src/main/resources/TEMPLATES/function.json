{
  "bindings" : [
    {
      "type" : "httpTrigger",
      "direction" : "in",
      "name" : "req",
      "route" : "{{ getCompleteURL }}",
      "methods" : [ "{{ getRestMethod }}" ],
      "authLevel" : "ANONYMOUS"
    }, {
      "type" : "http",
      "direction" : "out",
      "name" : "$return"
    }
  ]
}